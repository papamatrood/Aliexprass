{% extends 'base.html.twig' %}

{% block title %}New Address{% endblock %}

{% block body %}

{{ include("partials/title-section.html.twig", {'name':'Add Address'}) }}
<div class="login_register_wrap section">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-6 col-md-10">
                <div class="login_wrap">
                    <div class="padding_eight_all bg-white">
                        <div class="heading_s1">
                            <h3>Add Address</h3>
                        </div>
                        <div class="content">
                            {% for message_flash in app.flashes('message_flash') %}
                            <div class="alert alert-success" role="alert">{{ message_flash }}</div>
                            {% endfor %}
                            {{ include('address/_form.html.twig') }}
                            <a class="btn btn-fill-out btn-block" href="{{ path('account') }}">back to
                                list</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{% endblock %}